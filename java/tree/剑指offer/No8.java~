public class No8{
    public static void main(String[] args){
	int[] arr = {3,4,5,1,2};
	System.out.println(findMin(arr));
    }

    public static int findMin(int[] arr){
	int left = 0;
	int right = arr.length - 1;
	if (arr[right] > arr[left]){
	    try{
		throw new Exception("非旋转数组");
	    } catch (Exception e) {
		e.printStackTrace();
		return -1;
	    }
	}
		
     
	
